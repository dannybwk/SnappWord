{
  "version": 2,
  "functions": {
    "api/webhook.py": {
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py",
      "methods": ["POST"]
    }
  ]
}
